{
  "name": "yapp",
  "version": "0.1.0",
  "description": "Yet another Planning Poker app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run build",
    "build": "cross-env NODE_ENV=development parcel build ./src/index.html --no-cache --public-url ./dist/",
    "dev": "cross-env NODE_ENV=production parcel watch ./src/index.html --no-cache --public-url ./dist/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/selfdeceited/yapp.git"
  },
  "keywords": [
    "planningpoker",
    "socket"
  ],
  "author": "selfdeceited",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selfdeceited/yapp/issues"
  },
  "homepage": "https://github.com/selfdeceited/yapp#readme",
  "dependencies": {
    "babel-preset-react-app": "^3.1.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "parcel-bundler": "^1.5.1",
    "ramda": "^0.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.0",
    "cross-env": "^7.0.2",
    "jest": "^22.4.3"
  }
}
